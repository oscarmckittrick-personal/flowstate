{
  "expo": {
    "name": "flowstate",
    "slug": "flowstate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "flowstate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C1742"
    },
    "ios": {
      "bundleIdentifier": "com.oscarmckittrick.flowstate",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_foreground.png",
        "backgroundColor": "#0C1742"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.oscarmckittrick.flowstate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4514b85a-16f2-41e0-9e12-aeb888622d42"
      }
    },
    "owner": "oscarmckittrick"
  }
}
